{
  "name": "@abc-map/create-module",
  "version": "1.0.12",
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "main": "build/create-module.js",
  "bin": {
    "create-module": "build/create-module.js"
  },
  "files": [
    "build",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && chmod +x build/create-module.js && ln -sf ../../build/create-module.js ./node_modules/.bin/create-module",
    "clean": "rm -rf build",
    "clean-build": "yarn run clean && yarn run build",
    "dep-check": "depcruise -c .dependency-cruiser.cjs src",
    "lint": "prettier --loglevel warn --check . && eslint './src/**/{*.tsx,*.ts}' --max-warnings=0",
    "lint-fix": "prettier --loglevel warn --write . && eslint './src/**/{*.tsx,*.ts}' --max-warnings=0 --fix",
    "public-publish": "scripts/public-publish.sh",
    "test": "mocha --trace-warnings 'build/**/*.test.js'",
    "test:interactive": "mocha --watch --trace-warnings 'build/**/*.test.js'",
    "watch": "tsc --watch | sed -u 's/\\x1Bc//g'"
  },
  "dependencies": {
    "adm-zip": "^0.5.10",
    "axios": "^1.2.1",
    "chalk": "^5.2.0",
    "command-line-args": "^5.2.1",
    "hasbin": "^1.2.3",
    "loglevel": "^1.8.1",
    "sort-package-json": "^2.1.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/chai": "^4.3.4",
    "@types/command-line-args": "^5.2.0",
    "@types/hasbin": "^1.2.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.17",
    "@types/sinon": "^10.0.13",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "chai": "^4.3.7",
    "chalk": "^5.2.0",
    "dependency-cruiser": "^12.1.1",
    "eslint": "^8.30.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "mocha": "^10.2.0",
    "nanoid": "^4.0.0",
    "prettier": "^2.8.1",
    "sinon": "^15.0.1",
    "typescript": "^4.9.4",
    "uuid": "^9.0.0"
  },
  "volta": {
    "node": "18.12.1"
  }
}
