{
  "name": "@abc-map/server",
  "version": "1.0.0",
  "private": true,
  "license": "AGPL-3.0-or-later",
  "main": "build/main.js",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf build && rm -rf public",
    "clean-build": "pnpm run clean && pnpm run build",
    "coverage": "nyc mocha --trace-warnings 'build/**/*.test.js'",
    "dependency-check": "depcruise -c .dependency-cruiser.js src",
    "lint": "prettier --loglevel warn --check . && eslint './src/**/{*.tsx,*.ts}' --max-warnings=0 && sort-package-json --check",
    "lint:fix": "prettier --loglevel warn --write . && eslint './src/**/{*.tsx,*.ts}' --max-warnings=0 --fix && sort-package-json",
    "start": "nodemon --trace-warnings --watch build --watch resources ./build/main.js",
    "start:ci": "node ./build/main.js",
    "test": "ts-mocha --trace-warnings 'src/**/*.test.ts'",
    "test:interactive": "ts-mocha --watch --trace-warnings 'src/**/*.test.ts'",
    "watch": "tsc --watch | sed -u 's/\\x1Bc//g'",
    "write-version": "node scripts/version.js"
  },
  "dependencies": {
    "@abc-map/shared": ">1.0.0-rc",
    "@fastify/error": "^3.3.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/jwt": "^7.2.0",
    "@fastify/multipart": "^7.7.3",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/sensible": "^5.2.0",
    "@fastify/static": "^6.10.2",
    "@fastify/view": "^8.0.0",
    "accept-language-parser": "^1.5.0",
    "ajv": "8.12.0",
    "bson": "^4.7.0",
    "fast-xml-parser": "^4.2.5",
    "fastify": "4.21.0",
    "fastify-metrics": "^10.3.0",
    "glob": "^8.0.3",
    "handlebars": "^4.7.7",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "luxon": "^3.1.1",
    "mongodb": "^5.7.0",
    "nodemailer": "^6.8.0",
    "prom-client": "^14.1.0",
    "source-map-support": "^0.5.21",
    "uuid-random": "^1.3.2"
  },
  "devDependencies": {
    "@types/accept-language-parser": "^1.5.3",
    "@types/chai": "^4.3.4",
    "@types/geojson": "^7946.0.10",
    "@types/glob": "^8.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.191",
    "@types/luxon": "^3.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.17",
    "@types/nodemailer": "^6.4.7",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "chai": "^4.3.7",
    "dependency-cruiser": "^13.0.4",
    "eslint": "^8.39.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "form-data": "^4.0.0",
    "mocha": "^10.2.0",
    "mocha-chai-jest-snapshot": "^1.1.4",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0",
    "prettier": "^2.8.1",
    "sinon": "^15.0.1",
    "sort-package-json": "^2.4.1",
    "ts-mocha": "^10.0.0",
    "typescript": "5.0.4"
  },
  "docker": {
    "imageName": "server"
  }
}
